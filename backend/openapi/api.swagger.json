{
  "swagger": "2.0",
  "info": {
    "title": "EDA Workshop Api",
    "version": "1.0"
  },
  "tags": [
    {
      "name": "HealthService"
    },
    {
      "name": "FilesService"
    },
    {
      "name": "StorageService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/healthz": {
      "get": {
        "summary": "Health check",
        "description": "Returns the health status of the service.",
        "operationId": "HealthService_Health",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/healthHealthResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "Health"
        ]
      }
    },
    "/storage/confirm-upload": {
      "post": {
        "summary": "Confirm File Upload",
        "description": "Confirms that a file has been uploaded.",
        "operationId": "StorageService_ConfirmFileUpload",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/storageConfirmFileUploadRequest"
            }
          }
        ],
        "tags": [
          "Storage"
        ]
      }
    },
    "/storage/file-url/{fileKey}": {
      "get": {
        "summary": "Get File URL",
        "description": "Retrieves a pre-signed URL for downloading a file.",
        "operationId": "StorageService_GetFileUrl",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/storageGetFileUrlResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "fileKey",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "Storage"
        ]
      }
    },
    "/storage/files": {
      "get": {
        "summary": "Get Files",
        "description": "Retrieves a paginated list of files.",
        "operationId": "FilesService_GetFiles",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/storageGetFilesResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "pageNumber",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "Files"
        ]
      },
      "delete": {
        "summary": "Delete Files",
        "description": "Deletes multiple files by their keys.",
        "operationId": "FilesService_DeleteFiles",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "fileKeys",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          }
        ],
        "tags": [
          "Files"
        ]
      }
    },
    "/storage/upload-url": {
      "get": {
        "summary": "Get Upload URL",
        "description": "Generates a pre-signed URL for uploading files.",
        "operationId": "StorageService_GetUploadUrl",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/storageGetUploadUrlResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "Storage"
        ]
      }
    }
  },
  "definitions": {
    "corePagination": {
      "type": "object",
      "properties": {
        "pageNumber": {
          "type": "integer",
          "format": "int32"
        },
        "pageSize": {
          "type": "integer",
          "format": "int32"
        },
        "totalItems": {
          "type": "integer",
          "format": "int32"
        },
        "hasNextPage": {
          "type": "boolean"
        }
      }
    },
    "healthHealthResponse": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "storageConfirmFileUploadRequest": {
      "type": "object",
      "properties": {
        "fileName": {
          "type": "string"
        },
        "fileKey": {
          "type": "string"
        }
      }
    },
    "storageFile": {
      "type": "object",
      "properties": {
        "fileName": {
          "type": "string"
        },
        "fileKey": {
          "type": "string"
        },
        "fileSize": {
          "type": "string",
          "format": "int64"
        },
        "fileType": {
          "type": "string"
        },
        "createdAt": {
          "type": "string"
        }
      }
    },
    "storageGetFileUrlResponse": {
      "type": "object",
      "properties": {
        "fileUrl": {
          "type": "string"
        }
      }
    },
    "storageGetFilesResponse": {
      "type": "object",
      "properties": {
        "pagination": {
          "$ref": "#/definitions/corePagination"
        },
        "files": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/storageFile"
          }
        }
      }
    },
    "storageGetUploadUrlResponse": {
      "type": "object",
      "properties": {
        "uploadUrl": {
          "type": "string"
        },
        "fileKey": {
          "type": "string"
        }
      }
    }
  }
}
