{
  "swagger": "2.0",
  "info": {
    "title": "EDA Workshop Api",
    "version": "1.0"
  },
  "tags": [
    {
      "name": "HealthService"
    },
    {
      "name": "StorageService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/health": {
      "get": {
        "summary": "Health check",
        "description": "Returns the health status of the service.",
        "operationId": "HealthService_Health",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/healthHealthResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "Health"
        ]
      }
    },
    "/storage/confirm-upload": {
      "post": {
        "summary": "Confirm File Upload",
        "description": "Confirms that a file has been uploaded.",
        "operationId": "StorageService_ConfirmFileUpload",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/storageConfirmFileUploadRequest"
            }
          }
        ],
        "tags": [
          "Storage"
        ]
      }
    },
    "/storage/upload-url": {
      "get": {
        "summary": "Get Upload URL",
        "description": "Generates a pre-signed URL for uploading files.",
        "operationId": "StorageService_GetUploadUrl",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/storageGetUploadUrlResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "Storage"
        ]
      }
    }
  },
  "definitions": {
    "healthHealthResponse": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "storageConfirmFileUploadRequest": {
      "type": "object",
      "properties": {
        "fileName": {
          "type": "string"
        },
        "fileKey": {
          "type": "string"
        },
        "bucketName": {
          "type": "string"
        }
      }
    },
    "storageGetUploadUrlResponse": {
      "type": "object",
      "properties": {
        "uploadUrl": {
          "type": "string"
        },
        "bucketName": {
          "type": "string"
        },
        "fileKey": {
          "type": "string"
        }
      }
    }
  }
}
